{
  "name": "webpack-preact-boilerplate",
  "homepage": "http://ivbrajkovic.github.io/webpack-preact-boilerplate",
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ],
  "version": "1.0.0",
  "private": true,
  "description": "webpack preact boilerplate",
  "scripts": {
    "start": "webpack-dev-server --env.mode=development --config webpack.config.js --open",
    "start:local": "webpack-dev-server --port 3000 --host 0.0.0.0 --env.mode=development --config webpack.config.js --open",
    "dev": "webpack --config webpack.config.js",
    "build": "webpack --env.mode=production --config webpack.config.js",
    "analyze": "webpack --env.mode=production --env.analyze --config webpack.config.js",
    "deploy": "gh-pages -d dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.2",
    "file-loader": "^6.0.0",
    "gh-pages": "^2.2.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "style-loader": "^1.1.3",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "preact": "^10.4.0"
  }
}
