{
  "name": "webpack-preact-boilerplate",
  "homepage": "http://ivbrajkovic.github.io/webpack-preact-boilerplate",
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ],
  "version": "1.0.0",
  "private": true,
  "description": "webpack preact boilerplate",
  "scripts": {
    "start": "webpack-dev-server --env.NODE_ENV=development --config webpack.config.js --open",
    "start:local": "webpack-dev-server --port 3000 --host 0.0.0.0 --env.NODE_ENV=development --config webpack.config.js --open",
    "dev": "webpack --config webpack.config.js",
    "build": "webpack --env.NODE_ENV=production --config webpack.config.js",
    "analyze": "webpack --env.NODE_ENV=production --env.analyze --config webpack.config.js",
    "deploy": "gh-pages -d dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-regular-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "autoprefixer": "^9.7.4",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "file-loader": "^5.0.2",
    "gh-pages": "^2.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.9.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "style-loader": "^1.1.2",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "preact": "^10.2.1"
  }
}
